<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane maxWidth="300.0" minWidth="300.0" prefHeight="265.0" prefWidth="300.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="HornyMerk.prog2.exercises.set11.currencyconverter.CurrencyConverterCtrl">
   <children>
      <VBox prefHeight="265.0" prefWidth="300.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Label contentDisplay="CENTER" prefWidth="45.0" text="From">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox maxWidth="1.7976931348623157E308" prefWidth="195.0" fx:id="cbFrom">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ComboBox>
               </children>
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Label prefWidth="45.0" text="To">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox maxWidth="1.7976931348623157E308" prefWidth="195.0" fx:id="cbTo">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ComboBox>
               </children>
               <VBox.margin>
                  <Insets left="20.0" right="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Label prefWidth="45.0" text="Amount">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField maxWidth="1.7976931348623157E308" prefWidth="195.0" fx:id="tfAmount">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets left="20.0" right="20.0" />
               </VBox.margin>
            </HBox>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Convert" fx:id="btnConvert" onAction="#convertAction">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" />
               </VBox.margin>
            </Button>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0">
               <children>
                  <Label prefWidth="45.0" text="Result">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField editable="false" maxWidth="1.7976931348623157E308" prefWidth="195.0" fx:id="tfResult">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
